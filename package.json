{
  "name": "cuey",
  "version": "0.1.2",
  "description": "TypeScript client library for the Cuey REST API",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "test": "bun test",
    "prepublishOnly": "bun run build",
    "format": "prettier --write ."
  },
  "keywords": [
    "cuey",
    "api",
    "client",
    "typescript",
    "webhooks",
    "cron",
    "scheduler"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "^1.3.1",
    "@types/node": "^24.9.2",
    "dotenv": "^17.2.3",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TimurKr/cuey-ts"
  }
}
